{
    "name": "solumdesignum/deviation",
    "description": "laravel lightweight deviation monitoring tool.",
    "keywords": [
        "laravel",
        "deviation",
        "laravel-deviation",
        "laravel-deviation-monitoring",
        "laravel-errors-monitoring"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Oskars Germovs",
            "email": "oskars_germovs@inbox.lv",
            "homepage": "https://solum-designum.eu"
        }
    ],
    "homepage": "https://github.com/solumdesignum/deviation",
    "require": {
        "illuminate/support": "~5|~6|~7|~8|~9|~10",
        "php": "^7.4|^8.0|^8.1|^8.2",
        "barryvdh/laravel-dompdf": "^v2.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4|~5|~6|~7|~8|~9|~10",
        "orchestra/testbench": "~3|~4|~6|~7|~8"
    },
    "autoload": {
        "psr-4": {
            "SolumDeSignum\\Deviation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SolumDeSignum\\Deviation\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SolumDeSignum\\Deviation\\DeviationServiceProvider"
            ]
        }
    }
}
